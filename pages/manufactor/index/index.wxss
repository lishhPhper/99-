.title {
    float: left;
    width: 33%;
    text-align: center;
    font-size: 13pt;
    height: 50rpx;
    line-height: 50rpx;
    margin: 15rpx 0rpx;
}

.title-view {
    height: 80rpx;
    background-color: #f60;
}

.top-right-icon {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    top: 40rpx;
    right: 40rpx;
    background-color: #fff;
    border-radius: 50%;
}

.top-left-icon {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    top: 40rpx;
    left: 40rpx;
    background-color: #fff;
    border-radius: 50%;
}

.bottom-right-icon {
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    right: 40rpx;
    bottom: 50rpx;
    background-color: #fff;
    border-radius: 50%;
}

.image-list {
    width: 100%;
    margin: 5rpx 0rpx;
}

.header-item {
    height: 80rpx;
}

.header-item .warranty {
    float: left;
    height: 80rpx;
    margin: 0rpx 30rpx;
    line-height: 80rpx;
}

.header-item .warranty-image-view {
    float: left;
    height: 80rpx;
}

.header-item .warranty-image-item {
    width: 30rpx;
    height: 30rpx;
    margin: 25rpx 5rpx;
}

#cash-deposit {
    height: 80rpx;
}

#cash-deposit .text1 {
    float: right;
    height: 80rpx;
    line-height: 80rpx;
    margin: 0rpx 30rpx 0rpx 5rpx;
}

#cash-deposit .text2 {
    float: right;
    height: 80rpx;
    line-height: 80rpx;
    color: #f60;
}

#cash-deposit image {
    float: right;
    width: 40rpx;
    height: 40rpx;
    margin: 20rpx 20rpx;
}

.child-node {
    height: 60rpx;
    margin: 5rpx 0rpx;
}

.child-node view {
    float: left;
    width: 33%;
    text-align: center;
    color: #000;
    height: 60rpx;
    line-height: 60rpx;
}

.product-box {
    margin: 20rpx 20rpx;
}

.right-icon {
    float: right;
    height: 40rpx;
    width: 40rpx;
    margin: 20rpx 0rpx;
}

